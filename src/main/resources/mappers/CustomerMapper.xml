<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.car.model.dao.czh.CostomerMapper">

    <!--新增客户信息-->
    <insert id="insertCostomer">
    <selectKey keyProperty="crId" order="AFTER" resultType="java.lang.Integer">
        SELECT LAST_INSERT_ID()
    </selectKey>
        insert into customer values (#{crId},#{crName},#{crPhone},#{crIdNumber},#{crSex},#{crTime})
    </insert>

</mapper>